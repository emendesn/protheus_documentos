 		dbSelectArea("SPED300")
		dbSetOrder(1)
		If MsSeek(::DemonstrativoRTF:ID_ENT+STR(::DemonstrativoRTF:ORDEM,18)+DTOS(::DemonstrativoRTF:DTINI)+DTOS(::DemonstrativoRTF:DTFIM))
			RecLock("SPED300",.F.)
