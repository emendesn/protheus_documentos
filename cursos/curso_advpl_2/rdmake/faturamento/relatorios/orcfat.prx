	cDirf,;
	cCodRet,;
	oCodRet,;	
	dVencIss,; 	//Vencimento ISS
	aCodR,;
	cRecIss)	//Informa se recolhe o ISS ou nao


Local aTemp                                              
Local aOpcoes := {"Sim","Nao"}
Local oFornIss
Local oLojaIss                     
Local oVencIss
Local oDescri 
                                                                  
Local cDescri  := ""
