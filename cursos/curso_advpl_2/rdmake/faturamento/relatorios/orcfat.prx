//Local aOpcIss	:=	{"1="+STR0024,"2="+STR0025}


//DEFAULT dVencIss := CtoD("")
//DEFAULT aCodR	:=	{}

//DEFAULT cRecIss := "1"

If nTipo == 1
	If Empty(aNfCab) .OR. Empty(aNfCab[NF_IMPOSTOS])
		aTemp	:= {{"","",0,0,0}}
	Else
		aTemp	:= aNFCab[NF_IMPOSTOS]
	EndIf
Else
